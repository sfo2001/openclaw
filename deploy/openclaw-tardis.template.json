  "agents": {
    "defaults": {
      "workspace": "/home/node/clawd",
      "userTimezone": "Europe/Berlin",
      "timeFormat": "24",
      "includeTimeInPrompt": true,
      "memorySearch": {
        "provider": "openai",
        "model": "nomic-embed-text",
        "remote": {
          "baseUrl": "http://matrix:11434/v1/",
          "apiKey": "ollama-local"
        },
        "fallback": "none"
      },
      "model": {
        "primary": "ollama/cogito:32b",
        "fallbacks": ["ollama/cogito-64k", "ollama/mistral-small3.2-32k"]
      },
      "imageModel": {
        "primary": "ollama/mistral-small3.2-32k"
      },
      "sandbox": {
        "mode": "off",
        "scope": "session",
        "workspaceAccess": "none",
        "docker": {
          "image": "localhost:5000/openclaw-sandbox:latest",
          "readOnlyRoot": true,
          "network": "none",
          "capDrop": ["ALL"],
          "tmpfs": [
            "/tmp:size=64m,noexec,nosuid,nodev",
            "/var/tmp:size=32m,noexec,nosuid,nodev",
            "/run:size=16m,noexec,nosuid,nodev"
          ],
          "pidsLimit": 64,
          "memory": "512m",
          "memorySwap": "512m",
          "cpus": 1,
          "ulimits": {
            "nofile": { "soft": 1024, "hard": 2048 },
            "nproc": { "soft": 64, "hard": 128 }
          }
        },
        "browser": { "enabled": false },
        "prune": { "idleHours": 1, "maxAgeDays": 1 }
      }
    }
  },
  "gateway": {
    "mode": "local",
    "bind": "lan",
    "port": 18789,
    "auth": {
      "token": "REPLACE_WITH_SECURE_TOKEN"
    }
  },
  "tools": {
    "allow": [
      "read",
      "write",
      "edit",
      "web_search",
      "web_fetch",
      "image",
      "memory_search",
      "memory_get",
      "exec",
      "process",
      "canvas",
      "nodes",
      "cron"
    ],
    "deny": ["browser", "gateway"],
    "elevated": {
      "enabled": true,
      "allowFrom": { "webchat": ["*"], "telegram": [845094732] }
    },
    "exec": {
      "safeBins": ["curl", "jq"]
    },
    "web": {
      "search": {
        "enabled": true,
        "provider": "brave",
        "maxResults": 5
      },
      "fetch": {
        "enabled": true,
        "maxChars": 50000,
        "timeoutSeconds": 30
      }
    }
  },
  "vault": {
    "enabled": false,
    "proxies": {
      "openai": "http://vault:8081",
      "anthropic": "http://vault:8082",
      "deepgram": "http://vault:8083"
    }
  }
}
